{
    "name": "esarth/modularis",
    "description": "Modularis - A Modular PHP Framework",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "Modules\\": "modules/"
        }
    },
    "require": {
        "php": ">=8.0",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9",
        "monolog/monolog": "^2.3",
        "guzzlehttp/guzzle": "^7.3",
        "tecnickcom/tcpdf": "^6.4",
        "phpoffice/phpspreadsheet": "^1.18",
        "koolreport/core": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "post-install-cmd": [
            "php core/Env.php",
            "php scripts/install.php"
        ],
        "post-update-cmd": [
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Mikaelarthur",
            "email": "mikaelarthprojet@gmail.com"
        }
    ],
    "license": "ESARTH"
}
